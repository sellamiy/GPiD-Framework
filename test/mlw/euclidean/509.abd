(size auto)

(declare u0 v0 u v tmp as vars)
(reference u v)

(lambda eq (l r) (= l r))
(lambda neq (l r) (distinct l r))
(lambda ge (l r) (>= l r))
(lambda gt (l r) (> l r))
(lambda le (l r) (<= l r))
(lambda lt (l r) (< l r))

(lambda gcd (a b) (gcd a b))

(apply gcd (over vars vars) vars symmetric)

(declare 0 as vars)

(apply eq (over vars vars) bf symmetric)
(apply neq (over vars vars) bf symmetric)
(apply le (over vars vars) bf nonself)
(apply ge (over vars vars) bf nonself)
(apply lt (over vars vars) bf nonself)
(apply gt (over vars vars) bf nonself)

(extend bf)

