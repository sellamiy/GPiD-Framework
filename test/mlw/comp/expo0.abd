(size auto)

(declare n m p w as var)
(declare 0 1 as const)

(reference p w)

(lambda power (n m) (power n m))

(lambda eq (l r) (= l r))
(lambda neq (l r) (distinct l r))
(lambda ge (l r) (>= l r))
(lambda gt (l r) (> l r))
(lambda le (l r) (<= l r))
(lambda lt (l r) (< l r))

(apply power (over var var) powerd)

(copy const as var)

(apply eq (over var var) bf symmetric)
(apply le (over var var) bf nonself)
(apply lt (over var var) bf nonself)

(apply eq (over var powerd) bf)

(extend bf)
