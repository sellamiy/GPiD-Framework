(size auto)

(declare tick time i (from_int i) as var)
(declare 0 10 as const)

(reference time i)

(lambda power (n m) (power n m))
(lambda sqrt (v) (sqr v))

(lambda eq (l r) (= l r))
(lambda neq (l r) (distinct l r))
(lambda ge (l r) (>= l r))
(lambda gt (l r) (> l r))
(lambda le (l r) (<= l r))
(lambda lt (l r) (< l r))

(lambda iplus (l r) (+ l r))
(lambda imult (l r) (* l r))

(apply imult (over var var) var)
(copy const as var)

(apply eq (over var var) bf symmetric)
(apply le (over var var) bf nonself)
(apply lt (over var var) bf nonself)

(extend bf)
