(size auto)

(declare x y z as vars)
(reference x y z)

(lambda fact (n) (fact n))

(apply fact (over vars) vars)

(lambda times (l r) (* l r))

(apply times (over vars vars) vars symmetric)

(declare 0 vars)

(lambda eq (l r) (= l r))
(lambda neq (l r) (distinct l r))
(lambda ge (l r) (>= l r))
(lambda gt (l r) (> l r))
(lambda le (l r) (<= l r))
(lambda lt (l r) (< l r))

(apply eq (over vars vars) bf symmetric)
(apply neq (over vars vars) bf symmetric)
(apply le (over vars vars) bf nonself)
(apply lt (over vars vars) bf nonself)

(extend bf)

