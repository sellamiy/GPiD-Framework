module Oopsla27
 use int.Int
 use ref.Ref

 let main (n: int) (l: int) : unit
  requires { l > 0 }
 =
  for k = 1 to n - 1 do
   invariant { true }
   for i = l to n - 1 do
    invariant { true }
    ()
   done;

   for i = l to n - 1 do
    invariant { true }
    assert { 1 <= k }
   done
  done

end
