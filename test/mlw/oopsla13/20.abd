(size auto)

(lambda eq (l r) (= l r))
(lambda neq (l r) (distinct l r))
(lambda ge (l r) (>= l r))
(lambda gt (l r) (> l r))
(lambda le (l r) (<= l r))
(lambda lt (l r) (< l r))

(lambda minus (l r) (- l r))
(lambda mult (l r) (* l r))
(lambda plus (l r) (+ l r))
(lambda div (l r) (div l r))
(lambda mod (l r) (mod l r))
(lambda uminus (l) (- l))

(declare x0 y0 k i n m j x y as int)
(reference m j x y)

(declare x0 x as lv)
(declare y0 y as rv)

(apply plus (over lv rv) int2)

(copy int2 as int)

(declare 0 as int)

(apply eq (over int int) bf symmetric)
(apply neq (over int int) bf symmetric)
(apply le (over int int) bf nonself)
(apply lt (over int int) bf nonself)

(extend bf)

