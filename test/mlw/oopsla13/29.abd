(size auto)

(lambda eq (l r) (= l r))
(lambda neq (l r) (distinct l r))
(lambda ge (l r) (>= l r))
(lambda gt (l r) (> l r))
(lambda le (l r) (<= l r))
(lambda lt (l r) (< l r))

(lambda minus (l r) (- l r))
(lambda mult (l r) (* l r))
(lambda plus (l r) (+ l r))
(lambda div (l r) (div l r))
(lambda mod (l r) (mod l r))
(lambda uminus (l) (- l))

(declare a b c d x y as int)
(reference a b c d x y)
(declare 0 1 as int)

(apply plus (over int int) int)

(apply mod (over int (strict 2)) int)

(apply eq (over int int) bf symmetric)
(apply neq (over int int) bf symmetric)
(apply le (over int int) bf nonself)
(apply lt (over int int) bf nonself)

(extend bf)

