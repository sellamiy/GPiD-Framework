(size auto)

(lambda eq (l r) (= l r))
(lambda neq (l r) (distinct l r))
(lambda ge (l r) (>= l r))
(lambda gt (l r) (> l r))
(lambda le (l r) (<= l r))
(lambda lt (l r) (< l r))

(lambda plus (l r) (+ l r))
(lambda div (l r ) (div l r))

(declare n m x y as int)

(declare 0 1 as int)

(apply plus (over int int) int symmetric)

(apply div (over int int) int)

(apply eq (over int int) bf symmetric)
(apply neq (over int int) bf symmetric)
(apply le (over int int) bf nonself)
(apply ge (over int int) bf nonself)
(apply lt (over int int) bf nonself)
(apply gt (over int int) bf nonself)

(extend bf)

